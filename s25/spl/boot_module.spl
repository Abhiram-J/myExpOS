// setting up page table for init process
PTBR = PAGE_TABLE_BASE+20;

//Library
[PTBR+0] = 63;
[PTBR+1] = "0100";
[PTBR+2] = 64;
[PTBR+3] = "0100";

//Heap
[PTBR+4] = 78;
[PTBR+5] = "0110";
[PTBR+6] = 79;
[PTBR+7] = "0110";

//Code
[PTBR+8] = 65;
[PTBR+9] = "0100";
[PTBR+10] = 66;
[PTBR+11] = "0100";
[PTBR+12] = -1;
[PTBR+13] = "0000";
[PTBR+14] = -1;
[PTBR+15] = "0000";

//Stack
[PTBR+16] = 76;
[PTBR+17] = "0110";
[PTBR+18] = 77;
[PTBR+19] = "0110";


// init
[PROCESS_TABLE + 17] = 1;
[PROCESS_TABLE + 19] = 1;
[PROCESS_TABLE + 20] = CREATED;
[PROCESS_TABLE + 27] = 80;
[PROCESS_TABLE + 28] = 0;
[PROCESS_TABLE + 29] = 8*512;
[PROCESS_TABLE + 30] = PAGE_TABLE_BASE+20;
[PROCESS_TABLE + 31] = 10;

[76 * 512] = [65 * 512 + 1];

// initializing disk map entry for init process
[DISK_MAP_TABLE + 10*1 + 0] = 80; //user are page
[DISK_MAP_TABLE + 10*1 + 1] = -1;
[DISK_MAP_TABLE + 10*1 + 2] = 78; //Heap
[DISK_MAP_TABLE + 10*1 + 3] = 79;
[DISK_MAP_TABLE + 10*1 + 4] = 65; //code
[DISK_MAP_TABLE + 10*1 + 5] = 66;
[DISK_MAP_TABLE + 10*1 + 6] = -1;
[DISK_MAP_TABLE + 10*1 + 7] = -1;
[DISK_MAP_TABLE + 10*1 + 8] = 76; //stack
[DISK_MAP_TABLE + 10*1 + 9] = 77;

// setting all other states as terminated

alias pid R0;
alias j R1;
pid = 2;
while ( pid < 16) do
	[PROCESS_TABLE + pid*16 + 4] = TERMINATED;
	j = 0;
	while ( j <= 18) do
		[PAGE_TABLE_BASE + pid*20 + j] = -1;
		[PAGE_TABLE_BASE + pid*20 + j + 1] = "0000";
		j = j + 2;
	endwhile;

	pid = pid +1;
endwhile;


// initializing terminal status table and disk status table

[TERMINAL_STATUS_TABLE] = 0;
[DISK_STATUS_TABLE] = 0; 
[SYSTEM_STATUS_TABLE] = 1; 


// initializing memory free list

alias page R0;
page = 76 ;

while(page <= 82) do
	[MEMORY_FREE_LIST + page] = 1;
	page = page + 1;
endwhile;

while(page < 128) do
	[MEMORY_FREE_LIST + page] = 0;
    page = page + 1;
endwhile;

// initializing mem free count and wait mem count

[SYSTEM_STATUS_TABLE + 2] = 45;
[SYSTEM_STATUS_TABLE + 3] = 0;


// setting per process resource table for init

alias i R0;
i = 496 ;
while(i < 512 ) do
	[80*512 + i] = -1;
	[80*512 + i + 1] = -1;
	i = i + 1;
endwhile;

//initialize semaphore table
i = 0;
while ( i < MAX_SEM_COUNT ) do
	[SEMAPHORE_TABLE + i*4 + 0] = -1;
	[SEMAPHORE_TABLE + i*4 + 1] = 0;
	i = i + 1;
endwhile;

//initialize file status table

i = 0;
while( i < MAX_FILE_NUM ) do
	[FILE_STATUS_TABLE + i*4 + 0] = -1;
	[FILE_STATUS_TABLE + i*4 + 1] = -1;
	i = i + 1;
endwhile;

// initialize open file table
i = 0;
while( i < MAX_OPENFILE_NUM ) do
	[OPEN_FILE_TABLE + i*4 + 0] = -1;
	[OPEN_FILE_TABLE + i*4 + 1] = 0;
	i = i + 1;
endwhile;

//initialize buffer table
i = 0;
while( i < MAX_BUFFER ) do
	[BUFFER_TABLE + i*4 + 0] = -1;
	[BUFFER_TABLE + i*4 + 1] = 0;
	[BUFFER_TABLE + i*4 + 2] = -1;
	i = i + 1;
endwhile;


return;